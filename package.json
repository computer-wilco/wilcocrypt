{
  "name": "wilcocrypt",
  "version": "2.0.0",
  "description": "A encrypting tool",
  "keywords": [
    "encrypting"
  ],
  "license": "GPL-3.0-only",
  "author": "Wilco Joosen",
  "type": "module",
  "main": "dist/wilcocrypt.min.js",
  "bin": "dist/cli.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "npx -y Jelmerro/nus",
    "rollup": "rollup -c && node src/scripts/fix-cli-min-import.js && node src/scripts/chmod.js",
    "sea": "node --experimental-sea-config sea/sea-config.json && cp $(command -v node) sea/wilcocrypt && npx postject sea/wilcocrypt NODE_SEA_BLOB sea/wilcocrypt.sea --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2",
    "clean": "rm -rf dist && rm -rf sea/wilcocrypt*",
    "build": "npm run rollup && npm run sea"
  },
  "devDependencies": {
    "@msgpack/msgpack": "3.1.2",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-replace": "6.0.3",
    "@rollup/plugin-terser": "0.4.4",
    "commander": "14.0.2",
    "rollup": "4.53.5"
  }
}
